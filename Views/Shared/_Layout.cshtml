<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DogMed</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Poppins:300,400,500,700" rel="stylesheet">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap">
    <link href="~/lib/mdbootstrap/mdbootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/mdbootstrap/mdb.min.css" rel="stylesheet" />
    <!-- Plugin file -->
    <link href="~/lib/mdbootstrap/muli-range.min.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>
<header>
        <partial name="_NavbarPartial"/>
</header>
    <div class="container-fluid">
        <main role="main">
            <partial name="_NotificationPartial" />
            @RenderBody()
        </main>
    </div>

    @*<footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2020 - DogMedicationTracker - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>*@
    <script src="~/lib/mdbootstrap/js/jquery-3.4.1.min.js"></script>
    <script src="~/lib/mdbootstrap/js/popper.min.js"></script>
    <script src="~/lib/mdbootstrap/js/bootstrap.min.js"></script>
    <script src="~/lib/mdbootstrap/js/mdb.min.js"></script>
    <script src="~/lib/mdbootstrap/js/multi-range.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
